<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.active4j.hr.system.dao.SysDeptDao">

    <select id="getLeaderRoleIdByRole" resultType="java.lang.String">
		select
		id
		from
		sys_role
		where
		sys_role.ROLE_NAME = #{leaderRole}
	</select>

</mapper>
